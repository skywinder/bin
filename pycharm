#!/bin/bash
#Generated by JetBrains Toolbox 1.28.1.15219 at 2024-03-12T16:30:48.863796

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/pk/Library/Application Support/JetBrains/Toolbox/apps/PyCharm-C/ch-0/233.13763.11/PyCharm CE.app/Contents/MacOS/pycharm" $wait --args "${ideargs[@]}"