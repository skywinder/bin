#!/bin/bash
#Generated by JetBrains Toolbox 1.28.1.15219 at 2023-06-12T12:41:33.316776

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/pk/Library/Application Support/JetBrains/Toolbox/apps/AppCode/ch-0/231.9011.41/AppCode.app/Contents/MacOS/appcode" $wait --args "${ideargs[@]}"